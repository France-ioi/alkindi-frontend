{
  "name": "alkindi-r2-front",
  "version": "1.10.1",
  "description": "Alkindi competition round 2 front-end",
  "authors": [
    "Epixode"
  ],
  "scripts": {
    "build": "babel --minified --out-dir lib src && NODE_ENV=production jspm bundle lib/index.js build/bundle.js --minify",
    "build-dev": "babel --source-maps=true --out-dir lib src && jspm bundle --dev --no-mangle 'alkindi-frontend' - '[alkindi-frontend/**/*]' - '[alkindi-frontend/style.css!css]' build/dev-bundle.js",
    "watch": "babel --watch --source-maps=true --out-dir lib src",
    "start": "node backend/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "express": "^4.14.0",
    "pug": "^2.0.0-beta6",
    "systemjs": "^0.19.41"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "jspm": "^0.17.0-beta.32"
  },
  "jspm": {
    "name": "alkindi-frontend",
    "main": "index.js",
    "dependencies": {
      "array.prototype.fill": "npm:array.prototype.fill@^1.0.1",
      "babel-runtime": "npm:babel-runtime@^6.20.0",
      "bootstrap": "github:twbs/bootstrap@^3.3.7",
      "classnames": "npm:classnames@^2.2.5",
      "collections": "npm:collections@^5.0.5",
      "css": "github:systemjs/plugin-css@^0.1.32",
      "debug": "npm:debug@^2.6.0",
      "deepmerge": "npm:deepmerge@^1.3.1",
      "epic-component": "npm:epic-component@^1.0.2",
      "epic-linker": "npm:epic-linker@^1.3.3",
      "es5-sham-ie8": "npm:es5-sham-ie8@^1.0.1",
      "es5-shim": "npm:es5-shim@^4.5.9",
      "es6-promise": "npm:es6-promise@^4.0.5",
      "es6-shim": "npm:es6-shim@^0.35.2",
      "flatten": "npm:flatten@^1.0.2",
      "font-awesome": "npm:font-awesome@^4.7.0",
      "html5shiv": "npm:html5shiv@^3.7.3",
      "intersperse": "npm:intersperse@^1.0.0",
      "lodash": "npm:lodash@^4.17.2",
      "memoizejs": "npm:memoizejs@^0.1.1",
      "node-range": "npm:node-range@^0.1.0",
      "object.assign": "npm:object.assign@^4.0.4",
      "rc-collapse": "npm:rc-collapse@^1.6.11",
      "rc-tooltip": "npm:rc-tooltip@^3.4.2",
      "react-bootstrap": "npm:react-bootstrap@^0.30.7",
      "react-dnd-html5-backend": "npm:react-dnd-html5-backend@^2.1.2",
      "react-redux": "npm:react-redux@^4.4.6",
      "redux-devtools-dock-monitor": "npm:redux-devtools-dock-monitor@^1.1.1",
      "redux-devtools-log-monitor": "npm:redux-devtools-log-monitor@^1.1.1",
      "reselect": "npm:reselect@^2.5.4",
      "shuffle": "npm:shuffle@^0.2.2",
      "superagent": "npm:superagent@^3.1.0"
    },
    "devDependencies": {
      "module": "npm:jspm-nodelibs-module@^0.2.0",
      "source-map-support": "npm:source-map-support@^0.4.8"
    },
    "peerDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "child_process": "npm:jspm-nodelibs-child_process@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "domain": "npm:jspm-nodelibs-domain@^0.2.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "graceful-fs": "npm:graceful-fs@^4.1.6",
      "http": "npm:jspm-nodelibs-http@^0.2.0",
      "https": "npm:jspm-nodelibs-https@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "querystring": "npm:jspm-nodelibs-querystring@^0.2.0",
      "react": "npm:react@^15.0.0",
      "react-dnd": "npm:react-dnd@^2.1.4",
      "react-dom": "npm:react-dom@^15.4.1",
      "redux": "npm:redux@^3.6.0",
      "redux-devtools": "npm:redux-devtools@^3.3.1",
      "redux-saga": "npm:redux-saga@^0.12.1",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "url": "npm:jspm-nodelibs-url@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0",
      "zlib": "npm:jspm-nodelibs-zlib@^0.2.0"
    },
    "overrides": {
      "github:twbs/bootstrap@3.3.7": {
        "main": "js/bootstrap.js",
        "dependencies": {
          "jquery": "jspm:jquery@*"
        },
        "files": [
          "dist",
          "fonts",
          "js",
          "css"
        ],
        "meta": {
          "js/bootstrap.js": {
            "deps": [
              "jquery"
            ],
            "exports": "$",
            "format": "global"
          }
        }
      },
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:debug@2.6.0": {
        "main": "browser.js",
        "directories": {
          "lib": "src"
        },
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "./node.js": {
            "browser": "./browser.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:font-awesome@4.7.0": {
        "main": "css/font-awesome.css!",
        "jspmNodeConversion": false,
        "registry": "jspm",
        "dependencies": {
          "css": "0.1"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:jquery@3.1.1": {
        "format": "amd"
      },
      "npm:lodash@4.17.4": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:ms@0.7.2": {
        "jspmNodeConversion": false,
        "format": "cjs"
      }
    }
  }
}
